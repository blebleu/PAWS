        if(global.weapon_array[curGun, 12] &gt; 0)
        { 
            audio_play_sound(global.weapon_array[curGun, 8], 100, false);
            global.weapon_array[curGun, 12]--;
            var bullet_id;
            
            if(right)
            {
                bullet_id = instance_create(x + lengthdir_x(global.weapon_array[curGun, 4], image_angle) - 
